version: "3"
services:
  clash:
    build: 
      context: .
      dockerfile: Dockerfile-clash
    container_name: clash
    ports:
      - "7890:7890"
      - "9090:9090"
    restart: always
  dashboard:
    build: 
      context: .
      dockerfile: Dockerfile-dashboard
    container_name: clash
    ports:
      - "80:80"
    restart: always
    
